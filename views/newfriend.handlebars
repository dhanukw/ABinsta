            <div class = "container" style="margin-bottom:75px">
                {{#each friends}}
                <div class= "friends">
                    <img src="{{imageURL}}" class="img-responsive imageClip" >
                    <h3><a href="{{profileURL}}" class="namelisten">{{name}}</h3></a>
                    <label for="persondescription">{{description}}</label><p></p>
                    <label for="sharefilter">You can see {{name}}'s {{Filters}} filters.</label>
                    <hr></hr>
                </div>
                {{/each}}
             </div>
         </div>


