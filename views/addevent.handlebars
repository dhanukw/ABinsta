    <div class = "container">
        <form method = "post" action = "eventsuccess">
            <h3>Event Title</h3>
            <input name= "title" type="text" class="form-control" id="title" placeholder="Title" required autofocus>
            <h3>Privacy</h3>
            <select input type="text" class="form-control" placeholder="">
                <option>JSON1</option>
                <option>JSON2</option>
                <option>Following</option>
                <option>Select Friends</option>
            </select>
            <p></p>

            <div class="well2">
                <h3>Start time</h3>
                <div id="starttimepicker" class="input-append" placeholder="start time">
                   <input data-format="yyyy-MM-dd HH:mm:ss PP" type="text" name="starttime"></input>
                   <span class="add-on">
                      <i data-time-icon="icon-time" data-date-icon="icon-calendar">
                      </i>
                  </span>
                </div>
            </div>
          <script type="text/javascript">
              $(function() {
               $('#starttimepicker').datetimepicker({
                   language: 'en',
                   pick12HourFormat: true
               });
           });
          </script>

          <div class="well2">
            <h3>End time</h3>
            <div id="endtimepicker" class="input-append" placeholder="end time">
               <input data-format="yyyy-MM-dd HH:mm:ss PP" type="text" name ="endtime"></input>
               <span class="add-on">
                   <i data-time-icon="icon-time" data-date-icon="icon-calendar">
                   </i>
               </span>
            </div>
          </div>
           <script type="text/javascript">
           $(function() {
              $('#endtimepicker').datetimepicker({
                  language: 'en',
                  pick12HourFormat: true
              });
          });
           </script>
       <p></p>
   </div>

   <div class = "container" >
    <h3>Notes</h3>
