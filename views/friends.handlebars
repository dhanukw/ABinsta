 <div class="container" style= "margin-bottom:50px">

 
  <div class="input">
  <input id="filter" type="text" class="form-control" placeholder="Filter Friends">
</div><br>
  <!-- Classes Info Class -->
    <div data-spy="scroll" data-target="#navbar-example2" data-offset="0" class="scrollspy-example fixed-box">
    <!-- Content for classes -->

 
    <table class="table table-hover classmates">
      <thead>
        <tr>
          <th>Profile</th>
          <th>Name</th>
          <th>Filters</th>
        </tr>
      </thead>
      <tbody>
        
        {{#each friends}}
        <tr align="{{profileURL}}">
          <td><img src="{{imageURL}}" height="75px" width="75px" class="img-thumbnail"></td>
          <td><a href="{{profileURL}}"></a>{{name}}</td>
          <td>{{Filters}}</td>
        </tr>
         {{/each}}
        
      </tbody>
    </table>
     

  </div>
    <!-- End content for classes -->

      
