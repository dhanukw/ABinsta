
<!DOCTYPE html>
<html>
<head>
    <title>instaCal </title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href = "css/bootstrap.min.css" rel = "stylesheet">
    <link href = "css/styles.css" rel = "stylesheet">
    <link href='css/fullcalendar.css' rel='stylesheet' />
    <link href='css/fullcalendar.print.css' rel='stylesheet' media='print' />
    <script src='js/jquery.min.js'></script>
    <script src='js/jquery-ui.custom.min.js'></script>
    <script src='js/fullcalendar.min.js'></script>
    <script src = "js/bootstrap.js"></script>


    <div class = "navbar navbar-default navbar-static-top" role="navigation" style="margin-top:-40px">
        <div class = "container">

            <a href = "index" class = "navbar-brand">instaCal</a>
        </div>
    </div>

    <style>

    body {
        margin-top: 40px;
        text-align: center;
        font-size: 14px;
        font-family: "Lucida Grande",Helvetica,Arial,Verdana,sans-serif;
    }

    #calendar {
        width: 360px;
        margin: 0 auto;
    }
    </style>

</head>



<body>
    
    <h3>Comparing with {{name}}</h3>
    <div id='calendar'></div>

    <script type = "text/javascript">
   
   var source = new Array();
    source[0] = '/multi_source/brian1';
    source[1] = '/multi_source/brian2';
    source[2] = '/multi_source/brian3';

    var compareSource = new Array();
    compareSource[0] = 'compare1';
    compareSource[1] = 'compare2';
    
    $(document).ready(loadCal);



    function loadCal() {

    

        $('#calendar').fullCalendar({
            eventSources: [
            source[0],
            source[1],
            source[2],
            compareSource[0],
            compareSource[1]
            ],

            eventRender: function (event, element){
                element.attr('href', '#');
            },
            eventClick: function(calEvent, jsEvent, view) {
               window.open('event/' + calEvent.title, calEvent);
           }
       });
    }
    </script>



    <p></p>
    <div class="container" align= "center" style="margin-bottom:75px">
    </div>
</div>

</body>

<footer>
